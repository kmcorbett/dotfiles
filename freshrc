# freshshell.com
# See http://freshshell.com/readme for documentation.

# Update fresh itself
fresh freshshell/fresh bin/fresh --bin

# Fresh from local .dotfiles
fresh gitconfig --file
fresh bashrc --file
fresh bash_profile --file
fresh tmux.conf --file
fresh ssh-config --file=~/.ssh/config-example

fresh emacs/init.el --file=~/.emacs.d/init.el
fresh emacs/auto-complete-1.3.1 --file=~/.emacs.d/auto-complete-1.3.1/
fresh emacs/color-theme-6.6.0 --file=~/.emacs.d/color-theme-6.6.0/
fresh emacs/markdown-mode.el --file=~/.emacs.d/markdown-mode.el

# Verisk inits
# TODO: separate repo?
fresh measuresengine/dtinit --file=~/.dtinit
fresh measuresengine/bin/tunnel2code.sh --file=~/bin/tunnel2code.sh

